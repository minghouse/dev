<div>
	<script>
var paywall_metered_model = function() {
    this.init = function(artId) {
        if(GetCkValue("udngold") !== "") {
            return false;
        }
        if(memberOnly() !== "0") {
            return false;
        }
        if(cId() === "5597") {
            return false;
        }
        if(cId() === "5590" && scId() === "12509") {
            return false;
        }
        if(scId() === "11038") {
            return false;
        }
        if(art_add(artId)) {
            art_day();
            var rules = get_meter_rules();
            for(rule in rules) {
                if(rules[rule]["enable"]) {
                    if(art_limit(rules[rule]["days"]) >= rules[rule]["quota"]) {
                        setCookieValue("paywall_hit", 'Y', today_end(rules[rule]["days"]));
                        art_send(rules[rule]["days"].toString(), art_limit(rules[rule]["days"]));
                    }
                        setCookieValue("meter_limit_day"+rules[rule]["days"].toString(), art_limit(rules[rule]["days"]), today_end(rules[rule]["days"]));
                }
            }
        }
        // if(GetCkValue("paywall_hit") !== "") {
        //    setCookieValue("paywall_url", encodeURIComponent(document.getElementsByName("URL")[0].getAttribute('content')), today_end(1));
        // }
        return true;
    }
    this.test = function() {
        var expires = get_gmt_time(10);
        // console.log(expires);
        if(GetCkValue("meter_expires") === "") {
            setCookieValue("meter_expires", expires, expires);
            setCookieValue("meter_articleIds_test", "test", expires);
        } else {
            setCookieValue("meter_articleIds_test", "test2", GetCkValue("meter_expires"));
        }
        // art_send(1, 11);
    }
    var art_send = function(day, read) {
        $.ajax({
	    url: 'https://money.udn.com/common/meteredCnt/1001/'+day+'/'+read,
	    type: 'get',
	    datatype: 'text',
	    cache: false,
	    success: function(msg) {
	        console.log('ok');
	    }
        });
    }
    var art_add = function(artId) {
        var expires = get_gmt_time(7);
        // console.log(expires);
        var obj = {};
        var meter_articleIds = GetCkValue("meter_articleIds");
        var strArtId = artId.toString();
        if(meter_articleIds === "") {
            obj[strArtId] = 1;
            setCookieValue("meter_articleIds", JSON.stringify(obj), expires);
            setCookieValue("meter_expires", expires, expires);
            return true;
        } else {
            if(GetCkValue("meter_expires") === "") {
                setCookieValue("meter_expires", expires, expires);
            } else {
                expires = GetCkValue("meter_expires");
            }
            try {
                meter_articleIds = JSON.parse(meter_articleIds);
                ary_meter_articleIds = Object.keys(meter_articleIds);
                if(ary_meter_articleIds.indexOf(strArtId) !== -1) {
                    meter_articleIds[strArtId] += 1;
                    setCookieValue("meter_articleIds", JSON.stringify(meter_articleIds), expires);
                    return false;
                } else {
                    meter_articleIds[strArtId] = 1;
                    setCookieValue("meter_articleIds", JSON.stringify(meter_articleIds), expires);
                    return true;
                }
            } catch(e) {
                obj[strArtId] = 1;
                setCookieValue("meter_articleIds", JSON.stringify(obj), expires);
                return true;
            }
        }
    }
    var today_end = function(day) {
        return new Date(new Date(new Date().toLocaleDateString()).getTime() + 24 * 60 * 60 * 1000 * day - 1).toGMTString();
    };
    var get_gmt_time = function(day) {
        var day = day || 0;
        var nowTime = new Date();
        var time = nowTime.getTime();
        time += (86400 * day) * 1000;
        nowTime.setTime(time);
        return nowTime.toGMTString();
    };
    var get_yyyymmdd_old = function(day) {
        var day = day || 0;
        var dateList = [];
        for(var i=day; i>0; i--) {
            dateList.push(get_yyyymmdd(-i+1));
        }
        return dateList;
    };
    var get_yyyymmdd = function(day) {
        var day = day || 0;
        var nowTime = new Date();
        nowTime.setDate(nowTime.getDate() + day);
        var yyyy = nowTime.toLocaleDateString().slice(0,4)
        var MM = (nowTime.getMonth()+1<10 ? '0' : '')+(nowTime.getMonth()+1);
        var dd = (nowTime.getDate()<10 ? '0' : '')+nowTime.getDate();
        return yyyy+MM+dd;
    };
    var setCookieValue = function(keyname, value, expires) {
        document.cookie = keyname+'='+value+';path=/;domain=udn.com;expires='+expires;
    }
    var art_limit = function(startDay) {
        var artCnt = 0;
        var checkDateList = get_yyyymmdd_old(startDay);
        var meter_day_read_pages = GetCkValue("meter_day_read_pages");
        if(meter_day_read_pages === "") {
            return 0;
        }
        meter_day_read_pages = JSON.parse(meter_day_read_pages);
        for(date in checkDateList) {
            if(meter_day_read_pages[checkDateList[date]]) {
                artCnt += meter_day_read_pages[checkDateList[date]];
            }
        }
        return artCnt;
    }
    var art_day = function(day) {
        var day = day || 0;
        var yyyymmdd = get_yyyymmdd(day);
        var expires = get_gmt_time(7);
        var obj = {};
        var meter_day_read_pages = GetCkValue("meter_day_read_pages");
        if(meter_day_read_pages === "") {
            obj[yyyymmdd] = 1;
            setCookieValue("meter_day_read_pages", JSON.stringify(obj), expires);
            setCookieValue("meter_expires", expires, expires);
        } else {
            if(GetCkValue("meter_expires") === "") {
                setCookieValue("meter_expires", expires, expires);
            } else {
                expires = GetCkValue("meter_expires");
            }
            try {
                meter_day_read_pages = JSON.parse(meter_day_read_pages);
                ary_meter_day_read_pages = Object.keys(meter_day_read_pages);
                if(ary_meter_day_read_pages.indexOf(yyyymmdd) !== -1) {
                    meter_day_read_pages[yyyymmdd] += 1;
                } else {
                    meter_day_read_pages[yyyymmdd] = 1;
                }
                setCookieValue("meter_day_read_pages", JSON.stringify(meter_day_read_pages), expires);
            } catch(e) {
                obj[yyyymmdd] = 1;
                setCookieValue("meter_day_read_pages", JSON.stringify(obj), expires);
            }
        }
    }
    var get_meter_rules = function() {
        return [{
            "rule_id":"meter_limit_day7",
            "rule_name":"meter_limit_day7",
            "days":7,
            "quota":70,
            "enable_log":true, // ga fa money_log
            "action_type":0,
            "alert_delay_days":0,
            "enable":true,
            "weight":7,
        },{
            "rule_id":"meter_limit_day1",
            "rule_name":"meter_limit_day1",
            "days":1,
            "quota":9,
            "enable_log":true, // ga fa money_log
            "action_type":0,
            "alert_delay_days":0,
            "enable":true,
            "weight":1,
        }];
    }
};
var metered_model = new paywall_metered_model();
//metered_model.init(rId());
</script></div>
</main>
