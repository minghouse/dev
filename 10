<script>
googletag.cmd.push(function() { 
 var sizeMapping = googletag.sizeMapping(). 
            addSize([750, 200], ['fluid', [1, 1]]). 
            addSize([0, 0], ['fluid', [1, 1]]).
            build()

 googletag.defineSlot('/129853887/udn.com/money/Web/money-PcMobile-endtxt', [[1, 1], 'fluid'], 'div-gpt-ad-1564727433317-0')
   .defineSizeMapping(sizeMapping)
   .addService(googletag.pubads());

 googletag.display('div-gpt-ad-1564727433317-0'); 
});
</script>
<!-- 廣告拉伸 避免被google 吐的影響 -->
<script>
    document.addEventListener('DOMContentLoaded', function() {
    var timer = setInterval(checkDom, 1000);
    function checkDom () {
        let text_ad = document.querySelector('#div-gpt-ad-1564727433317-0 iframe') || null;
        if(text_ad){
            text_ad.height = '42';
            clearInterval(timer);
        }
    }
});
</script>
